{
	"openapi": "3.0.1",
	"info": {
		"title": "Servicios de PRESYA2",
		"description": "API para la gestión de Préstamos y Avales",
		"termsOfService": "https://www.igae.pap.hacienda.gob.es",
		"contact": {
			"name": "PRESYA2",
			"url": "https://www.igae.pap.hacienda.gob.es",
			"email": "presya2@igae.hacienda.gob.es"
		},
		"license": {
			"name": "Apache 2.0",
			"url": "http://www.apache.org/licenses/LICENSE-2.0.html"
		},
		"version": "1.0.0",
		"x-logo": {
      		"url": "https://www.igae.pap.hacienda.gob.es/sitios/igae/es-ES/PublishingImages/Presentacion-IGAE-100x100.gif",
      		"altText": "Logo de la API"
  		}
	},
	"servers": [
		{
			"url": "http://localhost:7001/Presya2/restPresya2",
			"description": "Servidor LOCAL"
		},		
		{
    		"url": "http://deswls.central.sepg.minhac.age:8000/Presya2/restPresya2",
    		"description": "Servidor DESARROLLO"
  		}
	],
	"security": [
		{
			"servicioToken": []
		}
	],
	"tags": [
		{
			"name": "Préstamos",
			"description": "Servicios para obtener la información de los préstamos"
		},
		{
			"name": "Anticipos Reembolsables",
			"description": "Servicios para manejar información de anticipos reembolsables"
		}
	],
	"paths": {
		"/anticiposReembolsables/alta": {
			"post": {
				"tags": [
					"Anticipos Reembolsables"
				],
				"summary": "Alta de anticipos reembolsables",
				"operationId": "altaAnticipoReembolsable",
				"parameters": [
					{
						"name": "Sistema",
						"in": "header",
						"description": "Sistema",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				],
				"requestBody": {
					"description": "Lista de anticipos",
					"content": {
						"application/json": {
							"schema": {
								"type": "array",
								"items": {
									"$ref": "#/components/schemas/Anticipo"
								}
							}
						}
					},
					"required": true
				},
				"responses": {
					"200": {
						"description": "La petición se ha procesado correctamente",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/RespPostAnticipo"
								},
								"example": {
									"anticipos": [
										{
											"mensaje": "El anticipo del registro 1 correspondiente al préstamo P35632 ha sido dado de alta correctamente.",
											"importe cancelado": 0,
											"importe exceso": 10000.55,
											"orden del registro": 1
										}
									]
								}
							}
						}
					}
				},
				"security": [
					{
						"servicioToken": []
					}
				]
			}
		},
		"/prestamos": {
			"get": {
				"tags": [
					"Préstamos"
				],
				"summary": "Consulta genérica de préstamos",
				"operationId": "getPrestamos",
				"parameters": [
					{
						"name": "sistema",
						"in": "header",
						"description": "Sistema",
						"required": true,
						"schema": {
							"type": "string"
						}
					},
					{
						"name": "limit",
						"in": "query",
						"description": "Cantidad máxima de resultados",
						"required": false,
						"schema": {
							"type": "string",
							"default": "20"
						}
					},
					{
						"name": "offset",
						"in": "query",
						"description": "Cantidad de resultados omitidos",
						"required": false,
						"schema": {
							"type": "string",
							"default": "0"
						}
					}
				],
				"responses": {
					"200": {
						"description": "OK",
						"content": {
							"*/*": {
								"schema": {
									"type": "array",
									"items": {
										"$ref": "#/components/schemas/PrestamoBasico"
									}
								}
							}
						}
					}
				},
				"security": [
					{
						"servicioToken": []
					}
				]
			}
		},		
		"/prestamos/{codigoPrestamo}": {
			"get": {
				"tags": [
					"Préstamos"
				],
				"summary": "Consulta puntual de un préstamo",
				"operationId": "getPrestamoById",
				"parameters": [
					{
						"name": "sistema",
						"in": "header",
						"description": "Sistema",
						"required": true,
						"schema": {
							"type": "string"
						}
					},
					{
						"name": "codigoPrestamo",
						"in": "path",
						"description": "Código del préstamo",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				],
				"responses": {
					"200": {
						"description": "OK",
						"content": {
							"*/*": {
								"schema": {
									"$ref": "#/components/schemas/Prestamo"
								}
							}
						}
					}
				},
				"security": [
					{
						"servicioToken": []
					}
				]
			}
		},
		"/prestamos/{codigoPrestamo}/garantias": {
			"get": {
				"tags": [
					"Préstamos"
				],
				"summary": "Consulta garantías del préstamo",
				"operationId": "getGarantiasPrestamoById",
				"parameters": [
					{
						"name": "sistema",
						"in": "header",
						"description": "Sistema",
						"required": true,
						"schema": {
							"type": "string"
						}
					},
					{
						"name": "codigoPrestamo",
						"in": "path",
						"description": "Código del préstamo",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				],
				"responses": {
					"200": {
						"description": "OK",
						"content": {
							"*/*": {
								"schema": {
									"type": "array",
									"items": {
										"$ref": "#/components/schemas/GarantiaPrestamos"
									}
								}
							}
						}
					}
				},
				"security": [
					{
						"servicioToken": []
					}
				]
			}
		},
		"/datosGeneralesPrestamo/{codigoPrestamo}": {
			"get": {
				"tags": [
					"Datos generales préstamo"
				],
				"summary": "Consulta puntual datos generales de un préstamo",
				"operationId": "datosGeneralesPrestamoPorNumPrestamo",
				"parameters": [
					{
						"name": "sistema",
						"in": "header",
						"description": "Sistema",
						"required": true,
						"schema": {
							"type": "string"
						}
					},
					{
						"name": "codigoPrestamo",
						"in": "path",
						"description": "Código del préstamo",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				],
				"responses": {
					"200": {
						"description": "OK",
						"content": {
							"*/*": {
								"schema": {
									"$ref": "#/components/schemas/DatosGeneralesPrestamo"
								}
							}
						}
					}
				},
				"security": [
					{
						"servicioToken": []
					}
				]
			}
		},
		"/anticiposReembolsables": {
			"get": {
				"tags": [
					"Anticipos Reembolsables"
				],
				"summary": "Consulta genérica de anticipos reembolsables",
				"operationId": "getAnticipos",
				"parameters": [
					{
						"name": "sistema",
						"in": "header",
						"description": "Sistema",
						"required": true,
						"schema": {
							"type": "string"
						}
					},
					{
						"name": "limit",
						"in": "query",
						"description": "Cantidad máxima de resultados",
						"required": true,
						"schema": {
							"type": "string",
							"default": "20"
						}
					},
					{
						"name": "offset",
						"in": "query",
						"description": "Cantidad de resultados omitidos",
						"required": true,
						"schema": {
							"type": "string",
							"default": "0"
						}
					}
				],
				"responses": {
					"200": {
						"description": "La petición se ha procesado correctamente",
						"content": {
							"application/json": {
								"schema": {
									"type": "array",
									"items": {
										"$ref": "#/components/schemas/AnticipoGet"
									}
								},
								"example": [
									{
										"numPrestamo": "P13682",
										"numOpcContable": "00160316310002011",
										"fechaIngreso": "30/10/2015",
										"impCancelado": "10,67",
										"impExceso": null
									},
									{
										"numPrestamo": "P13682",
										"numOpcContable": "00160316310002011",
										"fechaIngreso": "30/10/2015",
										"impCancelado": "10,67",
										"impExceso": null
									}
								]
							}
						}
					}
				},
				"security": [
					{
						"servicioToken": []
					}
				]
			}
		},
		"/anticiposReembolsables/{codigoPrestamo}": {
			"get": {
				"tags": [
					"Anticipos Reembolsables"
				],
				"summary": "Consulta puntual de anticipos reembolsables por préstamo",
				"operationId": "obtenerAnticipoReembolsablesPorPrestamo",
				"parameters": [
					{
						"name": "Sistema",
						"in": "header",
						"description": "Sistema",
						"required": true,
						"schema": {
							"type": "string"
						}
					},
					{
						"name": "codigoPrestamo",
						"in": "path",
						"description": "Código del préstamo",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				],
				"responses": {
					"200": {
						"description": "La petición se ha procesado correctamente",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/RespGetAnticipo"
								},
								"example": {
									"numPrestamo": "P13682",
									"anticipos": [
										{
											"numPrestamo": "P13682",
											"numOpcContable": "00160316310002011",
											"fechaIngreso": "30/10/2015",
											"impCancelado": "210,67",
											"impExceso": null
										},
										{
											"numPrestamo": "P13682",
											"numOpcContable": "00160316310002011",
											"fechaIngreso": "30/10/2015",
											"impCancelado": "10,67",
											"impExceso": null
										},
										{
											"numPrestamo": "P13682",
											"numOpcContable": "00160316310002011",
											"fechaIngreso": "30/10/2015",
											"impCancelado": "10,67",
											"impExceso": null
										}
									]
								}
							}
						}
					}
				},
				"security": [
					{
						"servicioToken": []
					}
				]
			}
		}
	},
	"components": {
		"schemas": {
			"Anticipo": {
				"type": "object",
				"properties": {
					"fechaContable": {
						"type": "string",
						"description": "Fecha contable",
						"example": "10/05/2023"
					},
					"fechaReintegro": {
						"type": "string",
						"description": "Fecha reintegro",
						"example": "10/05/2023"
					},
					"numPrestamo": {
						"type": "string",
						"description": "Número de préstamo",
						"example": "P35632"
					},
					"importe": {
						"type": "number",
						"description": "Importe del anticipo",
						"example": 10000.55
					},
					"numOpcContable": {
						"type": "string",
						"description": "Número de operación contable",
						"example": "00160321310001419"
					}
				},
				"description": "Anticipo Reembolsable"
			},
			"AnticipoPost": {
				"type": "object",
				"properties": {
					"mensaje": {
						"type": "string",
						"description": "mensaje",
						"example": "El anticipo del registro 1 correspondiente al préstamo PXXXXX ha sido dado de alta correctamente."
					},
					"importe cancelado": {
						"type": "number",
						"description": "importe cancelado",
						"example": 0
					},
					"importe exceso": {
						"type": "number",
						"description": "importe exceso",
						"example": 139581.35
					},
					"orden del registro": {
						"type": "integer",
						"description": "Posición registro respuesta",
						"format": "int32",
						"example": 1
					}
				}
			},
			"RespPostAnticipo": {
				"type": "object",
				"properties": {
					"anticipos": {
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/AnticipoPost"
						}
					}
				}
			},
			"DatosBasicosPrestamo": {
				"required": [
					"beneficiarios",
					"codigoServicio",
					"codigoSituacionPrestamo",
					"descripcion",
					"programaAyuda",
					"referencia",
					"tipoPrestamo"
				],
				"type": "object",
				"properties": {
					"descripcion": {
						"type": "string",
						"description": "Descripción del préstamo",						
						"example": "MEJORA DEL PROCESO DE MECANIZADO CON LA INCORPORACIÓN DE NUEVAS TECNOLOGÍAS"
					},
					"codigoSituacionPrestamo": {
						"type": "string",
						"description": "Código de la situación en la que se encuentra un prestamo",
						"example": "1"
					},
					"descripcionSituacionPrestamo": {
						"type": "string",
						"description": "Descripción de la situación en la que se encuentra un prestamo",
						"example": "ACTIVO CON PAGOS"
					},
					"codigoServicio": {
						"type": "string",
						"description": "Código del servicio asociado al préstamo",
						"example": "10"
					},
					"descripcionServicio": {
						"type": "string",
						"description": "Nombre del servicio asociado al préstamo",
						"example": "DIRECCION GENERAL DE INVESTIGACION"
					},
					"referencia": {
						"type": "string",
						"description": "Referencia externa del préstamo",
						"example": "RCI04000020190110"
					},
					"fechaConcesion": {
						"type": "string",
						"description": "Fecha de concesión del préstamo",
						"format": "date-time"
					},
					"programaAyuda": {
						"type": "string",
						"description": "Programa de ayuda al que estÃ¡ vinculado el préstamo",
						"example": "PLAN VIVE 2008/2010"
					},
					"interesMercado": {
						"type": "boolean",
						"description": "Indica si se aplica o no el interés de mercado al préstamo"
					},
					"plazoPrescripcion": {
						"type": "integer",
						"description": "Plazo de prescripción del préstamo en meses",
						"format": "int32",
						"example": 48
					},
					"tipoPrestamo": {
						"type": "string",
						"description": "Tipo de préstamo",
						"example": "N"
					},
					"beneficiarios": {
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/Tercero"
						}
					}
				},
				"description": "Datos básicos del préstamo"
			},
			"PrestamoBasico": {
				"required": [
					"codigo",
					"datosBasicos",
					"selfLink"
				],
				"type": "object",
				"properties": {
					"codigo": {
						"type": "string",
						"description": "Código del prestamo en PRESYA'2",
						"example": "PXXXXX"
					},
					"selfLink": {
						"type": "string",
						"description": "URI del préstamo",
						"format": "uri",
						"example": "/prestamos/PXXXXX"
					},
					"datosBasicos": {
						"$ref": "#/components/schemas/DatosBasicosPrestamo"
					}
				},
				"description": "Representa los datos mínimos de un préstamo"
			},
			"Tercero": {
				"required": [
					"denominacion",
					"nacionalidad"
				],
				"type": "object",
				"properties": {
					"codigoTercero": {
						"type": "string",
						"description": "Código del tercero",
						"example": "ES000S4757677H"
					},
					"selfLink": {
						"type": "string",
						"description": "URI del tercero.",
						"example": "/terceros/ES000S4757677H"
					},
					"denominacion": {
						"type": "string",
						"description": "Denominación del tercero",
						"example": "TERCERO CORRESPONDIENTE AL NIF/CIF: S4757677H"
					},
					"nacionalidad": {
						"type": "string",
						"description": "Código del país del tercero acorde a la ISO 3166-1."
					},
					"enConcursoAcreedores": {
						"type": "boolean",
						"description": "Indica si el tercero se encuentra en concurso de acreedores"
					},
					"direccionTercero": {
						"$ref": "#/components/schemas/TerceroDireccionTercero"
					}
				},
				"description": "Representación de un tercero de un préstamo (beneficiario, endosatario, avalista...)"
			},
			"TerceroDireccionTercero": {
				"required": [
					"codigoPostal",
					"direccion",
					"municipio",
					"pais",
					"provincia",
					"tipoVia"
				],
				"type": "object",
				"properties": {
					"pais": {
						"type": "string",
						"description": "Código del país del tercero acorde a la ISO 3166-1.",
						"example": "ESPAÑA"
					},
					"provincia": {
						"type": "string",
						"description": "Código INE de la provincia.",
						"example": "CÁDIZ"
					},
					"municipio": {
						"type": "string",
						"description": "Código INE del municipio.",
						"example": "ARCOS DE LA FRONTERA"
					},
					"tipoVia": {
						"type": "string",
						"description": "Código del tipo de vía",
						"example": "CL"
					},
					"direccion": {
						"type": "string",
						"description": "Dirección del tercero.",
						"example": "Una calle cualquiera"
					},
					"numero": {
						"type": "string",
						"description": "Número",
						"example": "24"
					},
					"escalera": {
						"type": "string",
						"description": "Escalera"
					},
					"piso": {
						"type": "string",
						"description": "Piso",
						"example": "3º"
					},
					"puerta": {
						"type": "string",
						"description": "Puerta",
						"example": "B"
					},
					"codigoPostal": {
						"type": "integer",
						"description": "Código postal.",
						"format": "int32",
						"example": 28001
					}
				},
				"description": "Dirección de notificación de las cartas de pago"
			},
			"DatosPrestamo": {
				"type": "object",
				"properties": {
					"datosEconomicos": {
						"$ref": "#/components/schemas/DatosPrestamoDatosEconomicos"
					},
					"anualidades": {
						"type": "array",
						"description": "Listado de las anualidades del préstamo",
						"items": {
							"$ref": "#/components/schemas/DatosPrestamoAnualidades"
						}
					},
					"cuadrosAmortizacion": {
						"type": "array",
						"description": "Listado de los cuadros de amortización del préstamo",
						"items": {
							"$ref": "#/components/schemas/DatosPrestamoCuadrosAmortizacion"
						}
					},
					"observaciones": {
						"type": "array",
						"description": "Listado con las observaciones del préstamo",
						"items": {
							"$ref": "#/components/schemas/DatosPrestamoObservaciones"
						}
					},
					"documentos": {
						"type": "array",
						"description": "Listado de documentos vinculados al préstamo. No incluye el binario del documento. Para recuperarlo, es necesario consultar individualmente cada documento.",
						"items": {
							"$ref": "#/components/schemas/DocumentoBasico"
						}
					},
					"revocaciones": {
						"type": "array",
						"description": "Listado de las revocaciones del préstamo",
						"items": {
							"$ref": "#/components/schemas/DatosPrestamoRevocaciones"
						}
					},
					"obligacionesPago": {
						"type": "array",
						"description": "Listado de las obligaciones de pago del préstamo",
						"items": {
							"$ref": "#/components/schemas/DatosPrestamoObligacionesPago"
						}
					},
					"garantias": {
						"type": "array",
						"description": "Listado de las garantías del préstamo",
						"items": {
							"$ref": "#/components/schemas/DatosPrestamoGarantias"
						}
					},
					"cancelacionesAnticipadas": {
						"type": "array",
						"description": "Listado de cancelaciones anticipadas",
						"items": {
							"$ref": "#/components/schemas/DatosPrestamoCancelacionesAnticipadas"
